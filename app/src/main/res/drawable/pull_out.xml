<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape
        android:shape="rectangle">
        <stroke
            android:width="1dp"
            android:color="?android:attr/colorPrimary"/>
    </shape>
</item>
    <item
        android:bottom="1dp">
        <shape
            android:shape="rectangle">
            <solid android:color="?android:attr/colorPrimaryDark"/>
        </shape>
    </item>
</layer-list>